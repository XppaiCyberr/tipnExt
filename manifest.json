{
  "manifest_version": 3,
  "name": "TipN - $TIPN x $BETR Tracker",
  "version": "1.81",
  "description": "Fetch $BETR data for your Farcaster account",
  "author": "XppaiCyber",
  "icons": {
    "128": "icons/favicon.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://base-mainnet.g.alchemy.com/*",
    "https://base.llamarpc.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "TipN"
  },
  "content_scripts": [
    {
      "matches": ["https://farcaster.xyz/*"],
      "js": ["ethers.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["ethers.js", "img/logo.gif"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
} 